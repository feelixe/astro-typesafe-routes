{
  "name": "astro-typesafe-routes",
  "type": "module",
  "description": "Astro Integration for typesafe URLs",
  "version": "4.1.0",
  "keywords": [
    "astro-integration"
  ],
  "author": "Felix EklÃ¶f",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/feelixe/astro-typesafe-routes"
  },
  "homepage": "https://github.com/feelixe/astro-typesafe-routes",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./link": {
      "import": "./dist/components/link/astro/astro-link.astro"
    },
    "./link/react": {
      "import": "./dist/components/link/react/react-link.js",
      "types": "./dist/components/link/react/react-link.d.ts"
    },
    "./path": {
      "import": "./dist/path.js",
      "types": "./dist/path.d.ts"
    },
    "./params": {
      "import": "./dist/params.js",
      "types": "./dist/params.d.ts"
    },
    "./standard-schema": {
      "import": "./dist/standard-schema.js",
      "types": "./dist/standard-schema.d.ts"
    }
  },
  "scripts": {
    "dev": "tsc -w --incremental --preserveWatchOutput",
    "build": "bun build.ts",
    "publish": "bun publish.ts"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "peerDependencies": {
    "astro": ">=4.14.0 <6.0.0",
    "typescript": "^5.0.0",
    "react": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "dependencies": {
    "fast-glob": "^3.3.2",
    "semver": "^7.6.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.7.0",
    "@types/bun": "latest",
    "@types/semver": "^7.5.8",
    "astro": "^5.0.0",
    "eslint": "9.x",
    "globals": "^15.8.0",
    "prettier": "^3.4.2",
    "prompts": "^2.4.2",
    "react": "^19.0.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "rc-v8",
    "valibot": "^1.0.0-rc.3"
  }
}