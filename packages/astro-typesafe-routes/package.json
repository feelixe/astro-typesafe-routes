{
  "name": "astro-typesafe-routes",
  "type": "module",
  "description": "Astro Integration for typesafe URLs",
  "version": "5.0.0",
  "keywords": ["astro-integration"],
  "author": "Felix Ekl√∂f",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/feelixe/astro-typesafe-routes"
  },
  "homepage": "https://github.com/feelixe/astro-typesafe-routes",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./create-route": {
      "import": "./dist/create-route.js",
      "types": "./dist/create-route.d.ts"
    },
    "./link": {
      "import": "./dist/components/link/astro/astro-link.astro"
    },
    "./link/react": {
      "import": "./dist/components/link/react/react-link.js",
      "types": "./dist/components/link/react/react-link.d.ts"
    },
    "./path": {
      "import": "./dist/path.js",
      "types": "./dist/path.d.ts"
    },
    "./standard-schema": {
      "import": "./dist/standard-schema.js",
      "types": "./dist/standard-schema.d.ts"
    }
  },
  "scripts": {
    "dev": "tsc -w --incremental --preserveWatchOutput",
    "build": "bun build.ts",
    "lint": "biome lint"
  },
  "files": ["dist", "README.md", "LICENSE", "package.json"],
  "peerDependencies": {
    "astro": "^5.0.0",
    "typescript": "^5.0.0",
    "react": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/bun": "latest",
    "astro": "catalog:",
    "prettier": "^3.4.2",
    "react": "^19.0.0",
    "typescript": "catalog:",
    "valibot": "catalog:"
  }
}
