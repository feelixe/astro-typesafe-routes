---
import { getParams } from "astro-typesafe-routes/params";
import Link from "astro-typesafe-routes/link";
import { $path } from "astro-typesafe-routes/path";
import { getSearch } from "astro-typesafe-routes";
import { createRoute } from "astro-typesafe-routes/create-route";
import { z } from "astro/zod";

const Route = createRoute({ routeId: "/imports" });

getParams(Astro, "/imports");

const search = getSearch(Astro, z.object({}));
---

<pre>{JSON.stringify(search)}</pre>

<Link to="/imports">link</Link>

<a href={$path({ to: "/imports" })}>link</a>
